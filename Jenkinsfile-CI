pipeline {
  agent any
  stages {
    stage('Checkout code') {
        steps {
				checkout([$class: 'GitSCM', branches: [[name: "develop"]], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[credentialsId: 'Jenkins-Github', url: "https://github.com/gopalagarwal3/DockerCICDDemo.git"]]])
            }
        }
    stage('Build docker images') {
      steps {
        sh 'docker-compose build'
      }
    }
    stage('Push result image') {
		  steps {
			withDockerRegistry(credentialsId: 'JenkinsDockerHub', url:'') {
			  //sh 'docker push gopalagarwal/result'
			  sh 'docker-compose push vote worker result'
			}
		}
    }
  }
}